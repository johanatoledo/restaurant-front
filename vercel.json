{
  "rewrites": [
    { "source": "/ingresar", "destination": "/ingresar.html" },
    { "source": "/ingresarUsuario", "destination": "/ingresarUsuario.html" },
    { "source": "/menu", "destination": "/menu.html" },
    { "source": "/404", "destination": "/404.html" },
    { "source": "/index", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}